{
  "name": "@airbnb/lunar-apollo",
  "version": "1.1.0",
  "description": "Apollo and GraphQL utilities.",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "sideEffects": false,
  "repository": "https://github.com/airbnb/lunar",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "BUILD=cjs babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./lib",
    "build:esm": "BUILD=esm babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./esm"
  },
  "peerDependencies": {
    "@airbnb/lunar": "^1.0.0",
    "graphql": "^14.1.0",
    "graphql-tag": "^2.10.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@airbnb/lunar-test-utils": "*",
    "@types/react-test-renderer": "^16.8.2",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "react": "^16.8.6",
    "react-test-renderer": "^16.8.6"
  },
  "dependencies": {
    "@types/graphql": "^14.2.2",
    "@types/lodash": "^4.14.134",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "lodash": "^4.17.11",
    "react-apollo": "^2.5.8",
    "utility-types": "^3.7.0"
  }
}
