{
  "name": "@airbnb/lunar-metrics",
  "version": "1.0.0-alpha.0",
  "description": "Metrics handling for Lunar.",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "sideEffects": false,
  "repository": "https://github.com/airbnb/lunar",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "BUILD=cjs babel --root-mode upward --extensions .ts,.tsx --copy-files ./src --out-dir ./lib",
    "build:esm": "BUILD=esm babel --root-mode upward --extensions .ts,.tsx --copy-files ./src --out-dir ./esm"
  },
  "peerDependencies": {
    "@airbnb/lunar": "^1.0.0-alpha.0"
  },
  "devDependencies": {
    "@airbnb/lunar": "^1.0.0-alpha.0",
    "@airbnb/lunar-test-utils": "*"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.2",
    "@types/new-relic-browser": "0.1072.1",
    "raven-js": "^3.27.0"
  }
}
