{
  "private": true,
  "name": "@airbnb/lunar-test-utils",
  "version": "1.0.2",
  "description": "Utilities for local testing.",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "sideEffects": false,
  "repository": "https://github.com/airbnb/lunar",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "BUILD=cjs babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./lib",
    "build:esm": "BUILD=esm babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./esm"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.12"
  }
}
