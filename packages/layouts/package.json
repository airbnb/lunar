{
  "name": "@airbnb/lunar-layouts",
  "version": "1.0.3",
  "description": "Pre-defined page layouts for Lunar.",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "sideEffects": false,
  "repository": "https://github.com/airbnb/lunar",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "BUILD=cjs babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./lib",
    "build:esm": "BUILD=esm babel --root-mode upward --extensions .ts,.tsx ./src --out-dir ./esm"
  },
  "peerDependencies": {
    "@airbnb/lunar": "^1.0.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@airbnb/lunar": "^1.1.2",
    "@airbnb/lunar-test-utils": "^1.0.2",
    "react": "^16.8.6"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.3",
    "@types/airbnb-prop-types": "^2.11.0",
    "airbnb-prop-types": "^2.12.0",
    "utility-types": "^3.5.0"
  }
}
